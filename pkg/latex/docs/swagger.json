                        "name": "create_latex_type",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "create_latex_type=3(上传创建)时的zip文件(只支持zip文件)。",
                        "name": "zip_file",
                        "in": "formData"
                    },
                    {
                        "type": "string",
                        "description": "create_latex_type=4(模板创建)时的模板名称",
                        "name": "template_name",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
