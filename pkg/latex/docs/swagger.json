                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "latex"
                ],
                "summary": "获取latex列表",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "状态 1=正常 2=归档 3=删除",
                        "name": "status",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "从第多少条起始",
                        "name": "offset",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "获取条数",
                        "name": "limit",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "创建时间 create_at; 最后更新时间update_at; 当前用户操作时间current_user_operation_time;",
                        "name": "sort_field",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "asc升序 desc降序",
                        "name": "sort",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "latex文档ID(非必填项)",
                        "name": "id",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "put": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "latex批量修改,单个修改(归档,丢弃到垃圾桶,删除)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "latex"
                ],
                "summary": "latex批量修改,单个修改(归档,丢弃到垃圾桶,删除)",
                "parameters": [
                    {
                        "type": "string",
                        "description": "latex ID列表(只要option=3的时 并且该latex为用户创建时,需要此参数)，以,分隔 例如 1,2,3",
                        "name": "latex_ids",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "user latex ID列表，以,分隔 例如 1,2,3 (只要option=3的时, 并且该latex不是用户创建，放入此参数。表示用户要离开该latex)",
                        "name": "user_latex_ids",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "1=恢复到正常状态;2=归档;3=丢弃到回收站;4=删除",
                        "name": "option",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "创建latex",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "latex"
                ],
